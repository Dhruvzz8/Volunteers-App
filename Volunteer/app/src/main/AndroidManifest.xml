<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.selab.volunteer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MyEvents"></activity>
        <activity android:name=".Report" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ForgotPassword"
            android:label="@string/ResetPassword"
            android:noHistory="true"
            android:parentActivityName=".Login" />
        <activity
            android:name=".SignUp"
            android:label="@string/SignUp"
            android:noHistory="true"
            android:parentActivityName=".Login" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".feedbacktab"
            android:label="Feedback"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Profile"
            android:label="Profile"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Settings" />
        <activity
            android:name=".MyEventsVolunteers"
            android:parentActivityName=".MyEventsVolunteers" />
        <activity
            android:name=".EventDescription"
            android:parentActivityName=".MyEventsVolunteers" />
        <activity android:name=".VolunteerEvents"></activity>
        <activity android:name=".EventDescription1"> </activity>
        <activity
            android:name=".EventDescription2"
            android:parentActivityName=".MyEventsVolunteers" />
        <activity
            android:name=".Approved"
            android:parentActivityName=".MyEventsVolunteers" />
        <activity
            android:name=".Waitlisted"
            android:parentActivityName=".MyEventsVolunteers" />
        <activity android:name=".List_of_Events" />
        <activity android:name=".DetailsActivity" />
        <activity android:name=".HostFinal" />
        <activity android:name=".CalenderActivity" />
        <activity android:name=".HostAnEvent" />
    </application>

</manifest>